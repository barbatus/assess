{
  "name": "botpress",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "migrate-sqlite": "prisma migrate dev --name init --schema=./prisma/schema.prisma",
    "generate-sqlite": "prisma generate --schema=./prisma/schema.prisma",
    "db": "run-s migrate-sqlite generate-sqlite",
    "dev": "run-s db server:dev",
    "client:dev": "vite",
    "server:dev": "tsx src/server/server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "serve": "NODE_ENV=production node --loader tsx --experimental-specifier-resolution=node src/server/server.ts",
    "preview": "vite preview",
    "postinstall": "prisma generate",
    "format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "reset:db": "prisma migrate reset -f"
  },
  "imports": {
    "@": "."
  },
  "prisma": {
    "seed": "node -r @swc-node/register prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@prisma/client": "^4.11.0",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-label": "^2.0.1",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-select": "^1.2.1",
    "@radix-ui/react-slot": "^1.0.1",
    "@radix-ui/react-tabs": "^1.0.3",
    "@swc-node/register": "^1.6.7",
    "@swc/core": "^1.3.80",
    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "@tanstack/bling": "^0.5.0",
    "@tanstack/react-query": "^4.28.0",
    "@tanstack/react-table": "^8.9.1",
    "@tanstack/table-core": "^8.9.3",
    "@types/connect": "^3.4.35",
    "axios": "1.3.4",
    "class-variance-authority": "^0.6.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "eslint": "^8.36.0",
    "express": "^4.18.2",
    "i18next": "^22.5.0",
    "lodash.set": "^4.3.2",
    "lucide-react": "^0.217.0",
    "memoizee": "^0.4.15",
    "prettier": "^2.8.7",
    "prisma": "^4.11.0",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-day-picker": "^8.7.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.9.0",
    "react-ssr-prepass": "^1.5.0",
    "react-use": "^17.4.0",
    "tailwind-merge": "^1.12.0",
    "vite-plugin-ssr": "^0.4.138",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@esbuild-kit/esm-loader": "^2.5.5",
    "@faker-js/faker": "^8.0.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/lodash.set": "^4.3.7",
    "@types/memoizee": "^0.4.8",
    "@types/node": "^18.15.7",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "serve-static": "^1.15.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^3.12.6",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
